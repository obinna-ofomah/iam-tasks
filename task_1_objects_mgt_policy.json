{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "Statement1",
			"Effect": "Allow",
			"Action": ["s3:ListAllMyBuckets"],
			"Resource": ["*"]
		},

		{
			"Sid": "Statement2",
			"Effect": "Allow",
			"Action": ["s3:ListBucket"],
			"Resource": ["arn:aws:s3:::*spark-job*"]
		},

		{
			"Sid": "Statement3",
			"Effect": "Allow",
			"Action": ["s3:DeleteObject"],
			"Resource": ["arn:aws:s3:::spark-job-data-input/dumps/*"]
		},

		{
			"Sid": "Statement4",
			"Effect": "Deny",
			"Action": ["s3:DeleteObject"],
			"Resource": ["arn:aws:s3:::spark-job-data-input/dumps/*.csv"]
		}
	]
}